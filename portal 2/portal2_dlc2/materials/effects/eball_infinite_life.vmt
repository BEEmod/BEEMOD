"UnlitGeneric"
{
	"$basetexture" "effects/comball/comball"
	"$additive" 1
	"$translucent" 1
	"$vertexcolor" 1
	"$surfaceprop" "energyball"
	"$color" "[0.3 0.7 0.3]"
	"Proxies"
		{
			"AnimatedOffsetTexture"
			{
				"animatedtexturevar" "$basetexture"
				"animatedtextureframenumvar" "$frame"
				"animatedtextureframerate" 25.00
			}
		}
}
